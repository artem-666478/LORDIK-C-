#include <iostream>
#include <vector>
using namespace std;

vector<int> graph[100];
bool visited[100];

void dfs(int v) {
    cout << v << " ";
    visited[v] = true;
    
    for(int i = 0; i < graph[v].size(); i++) {
        int next = graph[v][i];
        if(!visited[next]) {
            dfs(next);
        }
    }
}

int main() {
    graph[0].push_back(1);
    graph[0].push_back(2);
    graph[1].push_back(3);
    graph[1].push_back(4);
    graph[2].push_back(4);

    cout << "DFS: ";
    dfs(0);
    
    return 0;
}
